@model PriceFormVM

@section titleStart {
  Prices
}
@section titleSeceond {
    Create  Price
}

@section Header {
Prices
}
<div class="col-md-12">
    <div class="card card-primary">
        <div class="card-header">
            <h3 class="card-title">Create Price</h3>
        </div>
        <!-- /.card-header -->
        <!-- form start -->

        <form asp-controller="Prices" asp-action="Create">
            <div class="card-body">
                <div class="form-group">
                    <label asp-for="Name"> subscripe Name</label>
                    <input asp-for="Name" class="form-control" placeholder="Enter subscripe Name Required">
                </div>
                <div class="form-group">
                    <label asp-for="InitalPrice"> Initial Price</label>
                    <input asp-for="InitalPrice" class="form-control" placeholder="Enter Initial price value Required">
                </div>
            </div>
            <!-- /.card-body -->

            <div class="card-footer">
                <button type="submit" class="btn btn-primary">Create</button>
            </div>
        </form>
    </div>
</div>




